package Classes {		import flash.display.Sprite;	import flash.events.MouseEvent;	import flash.events.Event;	import Classes.Utilities.*;				public class AddDiscussion extends Sprite {				//variables to pass to discussion class		public var commentID:int;		public var userName:String;		public var userComment:String;				public function AddDiscussion(i:int, n:String, c:String) {						commentID = i;			userName = n;			userComment = c;			//tell content where to load			this.addedName_txt.text = userName;						this.addedComment_txt.text = userComment;					}								public function commentClick(e:MouseEvent):void{			//dispatch info to discuss page			dispatchEvent(new CustomEvent("commentClicked", commentID, userName, userComment));		}	}	}