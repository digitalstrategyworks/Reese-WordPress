package Classes {		import flash.display.SimpleButton;	import flash.events.MouseEvent;	import Classes.Utilities.CustomEvent;	import flash.events.Event;			public class SharkButton extends SimpleButton {		//pass variable that ids which button is clicked		public var sharkBtn: String;				public function SharkButton() {			//trace(this.name);			sharkBtn = this.name;			this.addEventListener(MouseEvent.CLICK, click);		}				public function click(e:MouseEvent):void{			//trace("BUTTON " + sharkBtn + " WAS CLICKED");			//sent information back to doc class based on which specific sharkbtn was clicked			dispatchEvent(new CustomEvent("huntMsg", sharkBtn));					}	}	}