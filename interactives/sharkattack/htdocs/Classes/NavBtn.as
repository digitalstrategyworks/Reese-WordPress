package Classes {		import flash.display.MovieClip;	import flash.events.MouseEvent;	import Classes.Utilities.CustomEvent;	import flash.events.Event;			public class NavBtn extends BasicButton {		//set vars re:navigation buttons to send to doc class		public var navID:int;		public var navTitle:String;				public function NavBtn(i:int, t:String) {			//code to make navbuttons work like buttons without cursor over text part			this.mouseChildren = false;						//define vars 			navID = i;			navTitle = t;			nav_txt.text = navTitle;			//trace(navTitle + "!!!!");		}				public override function click(e:MouseEvent):void{			//trace("NAVIGATION " + navID + " WAS CLICKED");			//send info to doc class based on which nav btn was selected			dispatchEvent(new CustomEvent("navMsg", navID, navTitle));					}	}	}