package Classes {	import flash.display.Sprite;	import flash.display.Loader;	import flash.events.MouseEvent;	import flash.net.URLLoader;	import flash.net.URLRequest;	import flash.events.Event;	import flash.events.IOErrorEvent;	import flash.events.ProgressEvent;	import Classes.Utilities.*;	import fl.transitions.Tween;	import fl.transitions.easing.Regular;	import fl.transitions.TweenEvent;			public class QuickBusiness extends Sprite {		public function QuickBusiness() {			//the whole reason for this class is basically to contain the tweens for hovering on the target signs on the locations section			this.addEventListener(MouseEvent.ROLL_OVER, mousehover);			this.addEventListener(MouseEvent.ROLL_OUT, mouseout);			info_mc.visible = false; //the info about each server farm isn't visible until you roll over it			mouseChildren = false;			target_mc.alpha = .6;					}			public function mousehover(e:MouseEvent):void {						var bwInTween:Tween = new Tween(target_mc, "width", Regular.easeInOut, target_mc.width, 45, .3, true);			var bhInTween:Tween = new Tween(target_mc, "height", Regular.easeInOut, target_mc.height, 45, .3, true);										var bInAlpha:Tween = new Tween(target_mc, "alpha", Regular.easeInOut, target_mc.alpha, 1, .3, true);					info_mc.visible = true; //the info about the server farm becomes visible		}				public function mouseout(e:MouseEvent):void {			info_mc.visible=false; //when you roll out, the info goes invisible again			var bwOutTween:Tween = new Tween(target_mc, "width", Regular.easeInOut, target_mc.width, 34, .3, true);			var bhOutTween:Tween = new Tween(target_mc, "height", Regular.easeInOut, target_mc.height, 34, .3, true);			var bOutAlpha:Tween = new Tween(target_mc, "alpha", Regular.easeInOut, target_mc.alpha, .6, .3, true);		}	}}